[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[user]
	name = Spencer Christensen
	email = 134820811+FreeSamples00@users.noreply.github.com
[core]
	excludesfile = /Users/sccmp/.globalgitignore
	pager = delta
